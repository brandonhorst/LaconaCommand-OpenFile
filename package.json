{
  "name": "lacona-finder",
  "version": "1.1.1",
  "description": "Lacona commands for opening apps, files, pref panes, and urls",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-finder.git"
  },
  "keywords": [
    "lacona",
    "open",
    "app",
    "application",
    "launch"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-finder/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-finder",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "rimraf": "^2.5.3"
  },
  "dependencies": {
    "elliptical": "^1.0.0",
    "lacona-api": "^1.0.0",
    "lacona-phrases": "^1.0.0",
    "lodash": "^4.13.1",
    "rxjs": "^5.0.0-beta.10"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-function-bind"
    ]
  }
}
